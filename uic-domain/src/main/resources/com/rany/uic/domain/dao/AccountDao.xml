    <select id="selectList" resultType="com.rany.uic.domain.po.AccountPO"
            parameterType="com.rany.uic.common.params.AccountSearchParam" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from account
        <where>
            <if test="tenantId != null">
                and tenant_id = #{tenantId}
            </if>
            <if test="accountName != null and accountName != ''">
                and account_name like concat('%', #{accountName}, '%')
